<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="Admin">
  	
<!--   	get blacklist -->
  	<select id="getBlackList" resultType="recoder.single.bangle.admin.DTO.BlackMember">
  		select b.seq "seq", id, b.addedDate "addeddate"
  		, b.reason "reason" from member m natural join blacklist b
  	</select>
  	
<!--   	unblock Blacklist -->
	<delete id="unblock">
		delete from blacklist where id = #{id}
	</delete>
 </mapper>